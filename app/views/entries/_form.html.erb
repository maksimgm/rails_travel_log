      <div id="error_explanation">
        <h2>
          <%= pluralize(@entry.errors.count, "error") %> prohibited
          this entry from being saved:
        </h2>
        <ul>
          <% @entry.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
          <% end %>
        </ul>
      </div>
    <% end %>
<%= form_for [@trip, @entry] do |f| %>
  
    <p>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
    </p>
    <p>
        <%= f.label :location %><br>
        <%= f.text_field :location, :class => 'search_location' %>
    </p>
    <p>
        <%= f.label :summary %><br>
    	<%= f.text_area :summary %>
	</p>
    <p>
        <%= f.label :cost %><br>
        <%= f.text_field :cost%>
    </p>
    <p>
        <%= f.label :image %><br>
        <%= f.text_field :image %>
    </p>
    <p>
        <%= f.label :video_url %><br>
        <%= f.text_field :video_url %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>