<%= form_for @user do |f|%>
  <%=f.label :username%>
  <%=f.text_field :username, class: "form-control" %>

  <!-- ADD UPLOADCARE API BELOW -->
  <%=f.label :profile_pic%>
  <%=f.image_tag :profile_pic, class: "form-control" %>

  <%=f.label :bio%>
  <%=f.text_field :bio, class: "form-control" %>

  <% f.submit class: "btn btn-primary" %>
<%end%>